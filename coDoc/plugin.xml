<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="CDC Editor"
            extensions="cdc"
            icon="icons/coDoc.gif"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            class="edu.pdx.svl.coDoc.editors.CDCEditor"
            id="edu.pdx.svl.coDoc.editors.CDCEditor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            name="coDoc"
            id="edu.pdx.svl.coDoc.commands.category">
      </category>
      <command
            name="Sample Command"
            categoryId="edu.pdx.svl.coDoc.commands.category"
            id="edu.pdx.svl.coDoc.commands.sampleCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="edu.pdx.svl.coDoc.commands.sampleCommand"
            class="edu.pdx.svl.coDoc.handlers.Handler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="edu.pdx.svl.coDoc.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="Sample Menu"
               mnemonic="M"
               id="edu.pdx.svl.coDoc.menus.sampleMenu">
            <command
                  commandId="edu.pdx.svl.coDoc.commands.sampleCommand"
                  mnemonic="S"
                  id="edu.pdx.svl.coDoc.menus.sampleCommand">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="edu.pdx.svl.coDoc.toolbars.sampleToolbar">
            <command
                  commandId="edu.pdx.svl.coDoc.commands.sampleCommand"
                  icon="icons/sample.gif"
                  tooltip="Say hello world"
                  id="edu.pdx.svl.coDoc.toolbars.sampleCommand">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="coDoc"
            icon="icons/coDoc.gif"
            class="edu.pdx.svl.coDoc.perspectives.Perspective"
            id="edu.pdx.svl.coDoc.perspectives.Perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            objectClass="org.eclipse.core.resources.IFile"
            id="edu.pdx.svl.coDoc.contribution1">
         <menu
               label="New Submenu"
               path="additions"
               id="edu.pdx.svl.coDoc.menu1">
            <separator
                  name="group1">
            </separator>
         </menu>
         <action
               label="New Action"
               class="edu.pdx.svl.coDoc.popup.actions.NewAction"
               menubarPath="edu.pdx.svl.coDoc.menu1/group1"
               enablesFor="1"
               id="edu.pdx.svl.coDoc.newAction">
         </action>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            name="coDoc"
            id="edu.pdx.svl.coDoc.views.category">
      </category>
      <view
            name="Property View"
            icon="icons/propertyview.gif"
            category="edu.pdx.svl.coDoc.views.category"
            class="edu.pdx.svl.coDoc.views.PropertyView"
            id="edu.pdx.svl.coDoc.views.PropertyView">
      </view>
   </extension>

</plugin>
